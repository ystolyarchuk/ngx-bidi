{
  "name": "ngx-bidi-workspace",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "ng build ngx-bidi && npm run copy:all",
    "copy:all": "cp ./projects/ngx-bidi/CHANGELOG.md dist/ngx-bidi/ && mkdir -p dist/ngx-bidi/scss && cp -r projects/ngx-bidi/src/lib/scss/* dist/ngx-bidi/scss/",
    "prepublish": "npm run copy:all",
    "copy": "cp ./projects/ngx-bidi/CHANGELOG.md dist/ngx-bidi/",
    "public": "cd dist/ngx-bidi && npm publish --access public"
  },
  "dependencies": {
    "@angular/common": "^20.3.15",
    "@angular/core": "^20.3.15",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@angular/cli": "^20.3.15",
    "@angular/compiler-cli": "^20.3.15",
    "@angular-devkit/build-angular": "^20.3.15",
    "ng-packagr": "^20.0.0",
    "typescript": "^5.4.0"
  }
}
